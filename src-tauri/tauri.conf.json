{
	"build": {
		"beforeBuildCommand": "npm run vite:build",
		"beforeDevCommand": "npm run vite:dev",
		"devPath": "http://localhost:1420",
		"withGlobalTauri": false,
		"distDir": "../build"
	},
	"package": {
		"productName": "Soundcheck",
		"version": "0.1.0"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"window": {
				"close": true,
				"create": true,
				"startDragging": true,
				"setAlwaysOnTop": true
			},
			"clipboard": {
				"writeText": true
			}
		},
		"bundle": {
			"active": true,
			"category": "Music",
			"targets": "all",
			"identifier": "Soundcheck",
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			]
		},
		"security": {
			"csp": null
		},
		"windows": [
			{
				"alwaysOnTop": true,
				"decorations": false,
				"fullscreen": false,
				"height": 175,
				"label": "player",
				"minHeight": 160,
				"minWidth": 160,
				"resizable": true,
				"title": "Soundcheck",
				"width": 360
			}
		]
	}
}
