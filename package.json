{
    "name": "soundcheck",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "npm run tauri dev",
        "build": "npm run tauri build",
        "test": "vitest",
        "tauri": "tauri",
        "vite:dev": "vite dev",
        "vite:build": "vite build",
        "svelte:check": "svelte-check --tsconfig ./tsconfig.json",
        "stylelint:check": "stylelint --ignore-path .gitignore **/*.{css,svelte}",
        "stylelint": "stylelint --fix --ignore-path .gitignore **/*.{css,svelte}",
        "prettier:check": "prettier --check --ignore-path .gitignore **/*.{css,html,ts,svelte}",
        "prettier": "prettier --write --ignore-path .gitignore **/*.{css,html,ts,svelte}",
        "eslint:check": "eslint --ignore-path .gitignore **/*.{ts,svelte}",
        "eslint": "eslint --fix --ignore-path .gitignore **/*.{ts,svelte}"
    },
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^2.5.2",
        "@testing-library/svelte": "^4.0.5",
        "@tsconfig/svelte": "^5.0.2",
        "@types/spotify-api": "^0.0.25",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.12.0",
        "eslint": "^8.53.0",
        "eslint-plugin-svelte": "^2.35.0",
        "jsdom": "^22.1.0",
        "postcss-html": "^1.5.0",
        "prettier": "^3.0.3",
        "prettier-plugin-svelte": "^3.1.0",
        "stylelint": "^15.11.0",
        "stylelint-config-html": "^1.1.0",
        "stylelint-config-idiomatic-order": "github:NDoolan360/stylelint-config-idiomatic-order",
        "stylelint-config-standard": "^34.0.0",
        "svelte": "^4.2.3",
        "svelte-check": "^3.6.0",
        "svelte-preprocess": "^5.1.0",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2",
        "vite": "^4.5.0",
        "vitest": "^0.34.6"
    },
    "dependencies": {
        "@material/material-color-utilities": "^0.2.7",
        "@square/svelte-store": "github:NDoolan360/svelte-store",
        "@tauri-apps/api": "^1.5.1",
        "@tauri-apps/cli": "^1.5.6",
        "tauri-plugin-store-api": "github:tauri-apps/tauri-plugin-store#v1",
        "tauri-plugin-window-state-api": "github:tauri-apps/tauri-plugin-window-state#v1"
    },
    "type": "module"
}
