{
	"name": "soundcheck",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "npm run tauri dev",
		"build": "npm run tauri build",
		"tauri": "tauri",
		"vite:dev": "vite dev",
		"vite:build": "vite build",
		"vite:preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
