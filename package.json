		"@tauri-apps/api": "^1.4.0",
		"@tauri-apps/cli": "^1.4.0",
		"@tsconfig/svelte": "^5.0.2",
		"@types/spotify-api": "^0.0.22",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.1",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.9"
	},
	"type": "module"
